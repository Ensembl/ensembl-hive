<?xml version='1.0' standalone='yes'?>
<pipeline>
  <hive_db>
    <dbname>lg4_long_mult</dbname>
    <host>localhost</host>
    <pass>ensembl</pass>
    <port>3306</port>
    <user>ensadmin</user>
  </hive_db>
  <pipeline_analyses>
    <analysis>
      <analysis_capacity>1</analysis_capacity>
      <batch_size>1</batch_size>
      <can_be_empty>0</can_be_empty>
      <failed_job_tolerance>0</failed_job_tolerance>
      <flow_rules>
        <flow>
          <dependent>
            <flow>
              <branch>2</branch>
              <target>part_multiply</target>
            </flow>
          </dependent>
          <target>add_together</target>
        </flow>
      </flow_rules>
      <logic_name>start</logic_name>
      <max_retry_count>3</max_retry_count>
      <meadow_type>LOCAL</meadow_type>
      <module>Bio::EnsEMBL::Hive::RunnableDB::LongMult::Start</module>
      <parameters>
        <param>
          <param_name>base</param_name>
          <param_value>10</param_value>
        </param>
      </parameters>
      <priority>0</priority>
      <rc_name>default</rc_name>
    </analysis>
    <analysis>
      <analysis_capacity>4</analysis_capacity>
      <batch_size>1</batch_size>
      <can_be_empty>0</can_be_empty>
      <failed_job_tolerance>0</failed_job_tolerance>
      <flow_rules>
        <flow>
          <target>:////intermediate_result</target>
        </flow>
      </flow_rules>
      <logic_name>part_multiply</logic_name>
      <max_retry_count>3</max_retry_count>
      <module>Bio::EnsEMBL::Hive::RunnableDB::LongMult::PartMultiply</module>
      <priority>0</priority>
      <rc_name>default</rc_name>
    </analysis>
    <analysis>
      <batch_size>1</batch_size>
      <can_be_empty>0</can_be_empty>
      <failed_job_tolerance>0</failed_job_tolerance>
      <flow_rules>
        <flow>
          <target>:////final_result</target>
        </flow>
      </flow_rules>
      <logic_name>add_together</logic_name>
      <max_retry_count>3</max_retry_count>
      <module>Bio::EnsEMBL::Hive::RunnableDB::LongMult::AddTogether</module>
      <priority>0</priority>
      <rc_name>default</rc_name>
      <wait_for_rules>
        <wait_for>part_multiply</wait_for>
        <wait_for>start</wait_for>
      </wait_for_rules>
    </analysis>
  </pipeline_analyses>
  <pipeline_parameters>
    <param>
      <param_name>pipeline_name</param_name>
      <param_value>long_mult</param_value>
    </param>
    <param>
      <param_name>take_time</param_name>
      <param_value>0</param_value>
    </param>
  </pipeline_parameters>
  <resource_mapping>
    <meadow>
      <meadow_type>LSF</meadow_type>
      <resources>
        <resource>
          <resource_name>urgent</resource_name>
          <resource_string>-q yesterday</resource_string>
        </resource>
        <resource>
          <resource_name>default</resource_name>
          <resource_string></resource_string>
        </resource>
      </resources>
    </meadow>
  </resource_mapping>
</pipeline>

